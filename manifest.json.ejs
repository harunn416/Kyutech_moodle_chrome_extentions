{
  "name": "九工大Moodle便利ツール",
  "version": "<%= packageVersion %>",
  "manifest_version": 3,
  "description": "<%= packageDescription %>",
  "content_scripts": [
    <% contentScripts.forEach((script, index) => { %>
    {
      "matches": <%= JSON.stringify(script.matches) %>,
      "js": <%= JSON.stringify(script.js) %>
      <% if (script.css) { %>,
      "css": <%= JSON.stringify(script.css) %>
      <% } %>
    }<%= index < contentScripts.length - 1 ? ',' : '' %>
    <% }); %>
  ],
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "permissions":[
    "storage"
  ],
  "web_accessible_resources": [
  {
    "resources": ["features.json"],
    "matches": ["<all_urls>"]
  }
]
}