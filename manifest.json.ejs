{
  "name": "九工大Moodle便利ツール",
  "version": "<%= packageVersion %>",
  "manifest_version": 3,
  "description": "<%= packageDescription %>",
  "content_scripts": [
    {
      "matches": ["https://*.kyutech.ac.jp/my/courses.php*"],
      "js": ["js/course_timetable.bundle.js"]
    },
    {
      "matches": ["https://*.kyutech.ac.jp/*"],
      "js": ["js/fun.bundle.js"]
    }
  ],
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "permissions":[
    "storage"
  ]
}